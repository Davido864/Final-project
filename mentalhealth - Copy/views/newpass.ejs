<!--This page allows you to enter a new password to login.-->

<!DOCTYPE html>

<html lang="en">

<head>

  <meta charset="UTF-8">

  <link rel="stylesheet" type="text/css" href="newpass.css" />

</head>

<body>

  <div class="container">

    <h1>Mental Health app</h1>

    <form action="/newpasssuccess" method="get">

      <input type="text" name="username" id="username" placeholder="Enter Username" required>

      <input type="password" name="password" id="password" placeholder="Enter password" required>

      <button type="submit" class="button-tailor" id="forgetButton">Enter your password</button>

    </form>

    <a href="/forgot" class="back-button">&larr;</a>

  </div>

</body>

</html>
